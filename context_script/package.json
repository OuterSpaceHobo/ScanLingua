{
  "name": "context_ts_react_esbuild",
  "private": true,
  "description": "Take screenshots",
  "version": "0.0.0",
  "manifest_version": 3,
  "type": "module",
  "background": {
    "service_worker": "index.js"
  },
  "permissions": [
    "desktopCapture",
    "downloads",
    "tabs"
  ],
  "action": {
    "default_title": "Take a Screenshot"
  },
  "icons": {
    "16": "/src/icon-16.png",
    "32": "/src/icon-32.png",
    "48": "/src/icon-48.png",
    "100": "/src/icon-100.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "scripts": {
    "build": "esbuild src/content-script.ts --bundle --minify --outfile=./../service_worker/public/content-script.js"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@google-cloud/translate": "^7.1.0",
    "@google-cloud/vision": "^3.1.0",
    "@types/chrome": "^0.0.217",
    "esbuild": "0.17.10",
    "framer-motion": "^10.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-frame-component": "^5.2.6",
    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "typescript": "^4.9.3"
  }
}
